#include <iostream>
using namespace std;

int main () {
    int pointsUser = 0;
    int pointsBatman = 0;
    int userInput;
    cout << "Who would win in a fight, you or Batman?" << endl;
    cout << "Answer these questions to find out!" << endl;
    cout << "On a scale of 1 to 10, how strong are you?" << endl;
    cin >> userInput;
    if (userInput > 7) {
        pointsUser += 1;
    }
    else {
        pointsBatman += 1;
    }
    return 0;
}
